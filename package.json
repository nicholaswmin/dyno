{
  "name": "@nicholaswmin/dyno",
  "type": "module",
  "version": "0.5.1",
  "description": "a multithreaded benchmarker",
  "main": "index.js",
  "bin": {
    "init": "bin/create-benchmark.js",
    "init-cloud": "bin/create-benchmark-cloud.js"
  },
  "scripts": {
    "test": "NODE_ENV=test node --test --no-warnings test/index.test.js",
    "test:coverage": "NODE_ENV=test node --test --no-warnings --experimental-test-coverage test/index.test.js",
    "checks": "npx eslint && npm audit",
    "examples:update": "bin/update-readme.js"
  },
  "engines": {
    "node": ">=22.6"
  },
  "keywords": [
    "test",
    "benchmark",
    "performance"
  ],
  "author": "@nicholaswmin",
  "license": "MIT",
  "devDependencies": {
    "globals": "^15.9.0"
  }
}
